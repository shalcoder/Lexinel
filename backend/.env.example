# AI API Keys
GOOGLE_API_KEY=your_primary_api_key
# Optional: Multiple keys for quota rotation
GOOGLE_API_KEYS=key1,key2,key3

# Model Configuration
# Available: gemini-3-flash-preview, gemini-3-pro-preview, gemini-2.0-flash, gemini-2.0-pro-exp-02-05
MODEL_FLASH=gemini-3-flash-preview
MODEL_PRO=gemini-3-pro-preview
EMBEDDING_MODEL=text-embedding-004

# Database & Persistence
# Set to 'true' to use Firebase Firestore, 'false' for local JSON
USE_FIREBASE=true
# Path to service account JSON or raw JSON string
FIREBASE_CREDENTIALS=serviceAccountKey.json

# System Settings

PROJECT_NAME=PolicyGuard AI
DEBUG=false
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
